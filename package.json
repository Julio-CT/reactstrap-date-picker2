{
  "name": "reactstrap-date-picker2",
  "version": "1.0.3",
  "description": "DatePicker for Reactstrap with extras",
  "main": "lib/index.js",
  "types": "dist/index.d.ts",
  "contributors": [
    "julio-ct"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julio-CT/reactstrap-date-picker2.git"
  },
  "keywords": [
    "datepicker",
    "reactstrap",
    "spanish",
    "disabledays",
    "days",
    "date",
    "picker",
    "react",
    "js"
  ],
  "author": "Julio-CT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Julio-CT/reactstrap-date-picker2/issues"
  },
  "homepage": "https://github.com/Julio-CT/reactstrap-date-picker2#readme",
  "files": [
    "lib",
    "dist"
  ],
  "cjs": "dist/reactstrap-date-picker.cjs.js",
  "browser": "dist/reactstrap-date-picker.umd.js",
  "module": "dist/reactstrap-date-picker.es.js",
  "scripts": {
    "clean-test": "rm -f test/bundle.js",
    "clean-lib": "rm -rf lib && mkdir lib",
    "clean-dist": "rm -rf dist && mkdir dist",
    "test": "npm run clean-test && rollup -c rollup.test.js && karma start --singleRun",
    "lib": "npm run clean-lib && babel src -d lib",
    "dist": "npm run clean-dist && rollup -c && cp src/*.d.ts dist/",
    "transpile": "tsc",
    "prepare": "npm run lint && npm run test && npm run lib && npm run dist",
    "demo": "rm -rf demo/dist && mkdir demo/dist && rollup -c rollup.demo.js -w",
    "format": "prettier --write src",
    "lint": "eslint --fix src -c .eslintrc.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.5.2",
    "co": "^4.6.0",
    "es6-promise": "^4.2.8",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^8.1.1",
    "node-sass": "^4.14.1",
    "node-uuid": "^1.4.8",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reactstrap": "^8.5.1",
    "rollup": "^2.23.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-postcss": "^3.1.5",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-terser": "^7.0.0",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.2",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "reactstrap": "^8.5.1"
  }
}
