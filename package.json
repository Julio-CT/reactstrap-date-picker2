{
  "name": "reactstrap-date-picker",
  "version": "0.0.9",
  "description": "Reactstrap based, zero dependencies, date picker",
  "author": "Donato Lorenzo <donato@afialapis.com>",
  "contributors": [
    "Donato Lorenzo <donato@afialapis.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/afialapis/reactstrap-date-picker.git"
  },
  "bugs": {
    "url": "https://github.com/afialapis/reactstrap-date-picker/issues"
  },
  "homepage": "https://reactstrap-date-picker.afialapis.com/",
  "files": [
    "lib",
    "dist"
  ],
  "main": "dist/reactstrap-date-picker.js",
  "browser": "dist/reactstrap-date-picker.umd.js",
  "module": "dist/reactstrap-date-picker.es.js",
  "scripts": {
    "old_lint": "eslint src",
    "old_test": "karma start test/karma.conf.js",
    "old_test-run": "karma start test/karma.conf.js --no-single-run",
    "old_build": "babel src -d lib",
    "old_clean-dist": "rimraf dist && mkdir dist",
    "old_build-dist": "webpack",
    "old_build-dist-min": "NODE_ENV=production webpack",
    "old_dist": "npm run clean-dist && npm run build-dist && npm run build-dist-min",
    "old_prepublish": "npm run lint && npm run test && npm run build && npm run dist",
    "old_demo": "NODE_ENV=development node demo/server",

    "clean-test": "rm -f test/bundle.js",
    "clean-lib": "rm -rf lib && mkdir lib",
    "clean-dist": "rm -rf dist && mkdir dist",
    "lint": "eslint src",
    "test": "npm run clean-test && rollup -c rollup.test.js && karma start --singleRun",
    "lib": "npm run clean-lib && babel src -d lib",
    "dist": "npm run clean-dist && rollup -c",
    "prepare": "npm run test && npm run lib && npm run dist && relative-deps",
    "demo": "rm -rf demo/dist && mkdir demo/dist && rollup -c rollup.demo.js -w"    
  },
  "keywords": [
    "js",
    "react",
    "reactstrap",
    "date",
    "picker"
  ],
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-arrow-functions": "7.10.1",
    "@babel/plugin-transform-classes": "7.10.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",        
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "co": "^4.6.0",
    "css-loader": "^3.2.0",
    "es6-promise": "^4.2.8",
    "eslint": "^6.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.2.0",
    "node-sass": "^4.12.0",
    "node-uuid": "^1.4.8",
    "rimraf": "^3.0.0",
    "rollup": "^2.23.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.5",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-terser": "^7.0.0",    
    "sass-loader": "^8.0.0",
    "terser-webpack-plugin": "^2.0.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "reactstrap": "^8.0.1"
  }
}
